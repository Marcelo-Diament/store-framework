{
    /*
     * 23.01 | SubMenu 01. Major
     * 'vtex.menu@2.x:menu#categories > vtex.menu@2.x:submenu#major > flex-layout.row#major'
     */
     "vtex.menu@2.x:submenu#major": {
        "children": ["flex-layout.row#major"]
    },
    "flex-layout.row#major": {
        "children": [
            "flex-layout.col#menu",
            "flex-layout.col#img"
        ]
    },

    "flex-layout.col#menu": {
        "children": [
            "vtex.menu@2.x:menu#major"
        ]
    },

    "flex-layout.col#img": {
        "children": [
            "image#menu",
            "rich-text#header"
        ],
        "props": {
            "paddingRight": 4,
            "horizontalAlign": "right"
        }
    },

    "image#menu": {
        "props": {
            "src": "https://appliancetheme.vteximg.com.br/arquivos/menu-washer.jpg",
            "link": {
                "url": "/small-appliances/coffee-makers"
            },
            "alt": "Coffee Makers Collection",
            "maxWidth": "200px"
        }
    }
}